{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["cypress", "node"],  // Ensure this is here
    "baseUrl": "./",
    "paths": {
      "@/*": ["cypress/*"]
    }
  },
  "include": [
    "cypress/**/*.ts",
    "cypress.config.ts",
    "cypress/support/commands.d.ts", // Ensure this file is included
    "cypress/support/commands.ts" // Ensure commands.ts is included
  ],
  "exclude": ["node_modules"]
}
